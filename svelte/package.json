{
	"name": "my-app",
	"version": "0.0.1",
	"scripts": {
		"dev": "vite dev --host 0.0.0.0",
		"build": "vite build",
		"package": "svelte-kit package",
		"prepare": "svelte-kit sync && npm run smui-theme-light && npm run smui-theme-dark",
		"smui-theme-light": "smui-theme compile static/smui.css -i src/theme",
		"smui-theme-dark": "smui-theme compile static/smui-dark.css -i src/theme/dark",
		"check": "svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --check --plugin-search-dir=. .",
		"format": "prettier --write --plugin-search-dir=. ."
	},
	"devDependencies": {
		"@material/elevation": "^14.0.0",
		"@material/typography": "^13.0.0",
		"@mdi/js": "^6.9.96",
		"@smui/banner": "^6.0.0-beta.16",
		"@smui/button": "^6.0.0-beta.16",
		"@smui/card": "^6.0.0-beta.16",
		"@smui/checkbox": "^6.0.0-beta.16",
		"@smui/drawer": "^6.0.0-beta.16",
		"@smui/form-field": "^6.0.0-beta.16",
		"@smui/layout-grid": "^6.0.0-beta.16",
		"@smui/list": "^6.0.0-beta.16",
		"@smui/paper": "^6.0.0-beta.16",
		"@smui/top-app-bar": "^6.0.0-beta.16",
		"@sveltejs/adapter-auto": "next",
		"@sveltejs/adapter-static": "^1.0.0-next.35",
		"@sveltejs/kit": "next",
		"prettier": "^2.6.2",
		"prettier-plugin-svelte": "^2.7.0",
		"smui-theme": "^6.0.0-beta.16",
		"svelte": "^3.44.0",
		"svelte-check": "^2.7.1",
		"svelte-preprocess": "^4.10.6",
		"tslib": "^2.3.1",
		"typescript": "^4.7.2",
		"vite": "^2.9.13",
		"vite-plugin-pwa": "^0.12.2"
	},
	"type": "module"
}
